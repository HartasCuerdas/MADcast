{% assign time_offset = "0|60|120|180|240|300|360" | split:"|" %}
{% for post in site.posts %}
  {% for segment in (1..{{post.segments}}) %}
    <item>
      <title>
        <![CDATA[
        {{ post.date | date: "%Y-%m-%d" }} - Parte {{ segment }}
        ]]>
      </title>
      <link>
        https://archive.org/details/lfdlp-{{ post.date | date: "%Y-%m-%d" }}
      </link>
      <enclosure url="https://archive.org/download/lfdlp-{{ post.date | date: "%Y-%m-%d" }}/parte-{{ segment }}.mp3" type="audio/mpeg" length="{{ post.lengths[forloop.index0] }}" />
      <description>
        <![CDATA[
        {{ post.date | date: "%Y-%m-%d" }} - Parte {{ segment }}
        ]]>
      </description>
      {% assign algo = 600  %}
      <pubDate>{{ post.pubDate | date: "%s" | plus : time_offset[forloop.index0] | date_to_rfc822 }}</pubDate>
      <guid>https://archive.org/download/lfdlp-{{ post.date | date: "%Y-%m-%d" }}/parte-{{ segment }}.mp3</guid>
    </item>
  {% endfor %}
